{
  "expo": {
    "name": "watcha",
    "slug": "watcha",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.toonlab.watcha"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android":{
      "package":"com.toonlab.watcha",
      "useNextNotificationsApi": true
    }
  }
}
